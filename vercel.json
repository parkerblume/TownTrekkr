{
    "version": 2,
    "builds": [
        {
            "src": "client/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "build"
            }
        }
    ],
    "routes": [
        { 
            "src": "/(.*)", 
            "dest": "client/$1" 
        }
    ]
}